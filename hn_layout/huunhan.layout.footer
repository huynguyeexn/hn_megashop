   <!-- STICKY,SHOW HIDE NAVBAR SCRIPT -->
   <script>
      window.onscroll = function() {
          myFunction()
      };
      var top_header = document.getElementsByClassName("hn-top-header")[0];
      var navbar = document.getElementById("js-sticky");
      var sticky = navbar.offsetTop + 40;
      
      function myFunction() {
          if (window.pageYOffset >= sticky) {
              navbar.classList.add("sticky"), top_header.style.padding = "30px 0px 80px 0px", navbar.style.transform = "translateY(0%)"
          } else {
              navbar.classList.remove("sticky"), top_header.style.padding = "30px 0px 0px 0px";
              navbar.style.transform = "translateY(50%)"
          }
      }
      
      function hide_navbar() {
          var a = document.getElementById("navbarToggleExternalContent");
          a.classList.remove("show")
      };
   </script>
